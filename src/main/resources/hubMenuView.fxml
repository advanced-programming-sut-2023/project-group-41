<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="314.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stronghold.controller.graphical.HubMenuController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="-2.0" prefHeight="331.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label text="Stronghold">
               <font>
                  <Font size="24.0" />
               </font>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </Label>
            <HBox alignment="CENTER" prefHeight="25.0" prefWidth="600.0">
               <children>
                  <Label text="Welcome, " />
                  <Label fx:id="usernameLabel" text="###USERNAME####" />
               </children>
            </HBox>
            <Button fx:id="startNewGameButton" mnemonicParsing="false" text="Start New Game" />
            <Button fx:id="profileMenuButton" mnemonicParsing="false" text="Profile Menu" />
            <Button fx:id="logOutButton" mnemonicParsing="false" onMouseClicked="#logOut" text="Log Out" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
